<?xml version="1.0" encoding="utf-8" ?>

<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="enum" invoke="manual">
		<point name="enumname">
			<text>
				MyEnum
			</text>
			<hint>
				enum name
			</hint>
		</point>
		<point name="one">
			<text>
				meOne
			</text>
			<hint>
				enum element
			</hint>
		</point>
		<description>
			enumeration declaration
		</description>
		<author>
			Embarcadero
		</author>
		<code language="Delphi" context="typedecl" delimiter="|"><![CDATA[T|enumname| = (|one|, |end|);]]>
		</code>
	</template>
</codetemplate>
